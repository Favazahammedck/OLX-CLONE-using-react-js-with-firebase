iv\">\n        <div className=\"brandName\" onClick={()=>{history.push('/')}}>\n          <OlxLogo></OlxLogo>\n        </div>\n        <div className=\"placeSearch\">\n          <Search></Search>\n          <input type=\"text\" />\n          <Arrow></Arrow>\n        </div>\n        <div className=\"productSearch\">\n          <div className=\"input\">\n            <input\n              type=\"text\"\n              placeholder=\"Find car,mobile phone and more...\"\n            />\n          </div>\n          <div className=\"searchAction\">\n            <Search color=\"#ffffff\"></Search>\n          </div>\n        </div>\n        <div className=\"language\">\n          <span> ENGLISH </span>\n          <Arrow></Arrow>\n        </div>\n        <div className=\"loginPage\">\n          <span onClick={()=>{history.push('/login')}}>{user ? user.displayName : 'Login'}</span>\n          <hr />\n        </div>\n        {user && <a onClick={()=>{\n          firebase.auth().signOut();\n          history.push('/login')\n        }}>Logout</a>}\n\n        <div className=\"sellMenu\">\n          <SellButton></SellButton>\n          <div className=\"sellMenuContent\">\n            <SellButtonPlus></SellButtonPlus>\n            <span  onClick={()=>{\n              {!user?history.push('/login') : history.push('/create')}\n            }}>SELL</span>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default Header;\n","/Users/arif/Desktop/React/untitled folder/React_tutorial_olx_clone/src/Components/Footer/Footer.js",[],"/Users/arif/Desktop/React/untitled folder/React_tutorial_olx_clone/src/assets/Search.js",[],"/Users/arif/Desktop/React/untitled folder/React_tutorial_olx_clone/src/assets/OlxLogo.js",[],"/Users/arif/Desktop/React/untitled folder/React_tutorial_olx_clone/src/assets/SellButtonPlus.js",[],"/Users/arif/Desktop/React/untitled folder/React_tutorial_olx_clone/src/assets/Heart.js",[],"/Users/arif/Desktop/React/untitled folder/React_tutorial_olx_cl